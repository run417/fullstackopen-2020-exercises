{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-else-return": "off"
    },
    "parserOptions": {
        "ecmaVersion": 11
    }
}
